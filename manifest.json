{
  "manifest_version": 2,

  "name": "cookie monster",
  "description": "Analyzes the cookies of a site and tries to fingerprint the backend based on them.",
  "version": "0.0.1.1",

  "permissions": [
    "cookies",
    "<all_urls>",
    "tabs",
    "storage"
  ],

  "icons" : {
    "48": "icon48.png",
    "128": "icon48.png"
  },

  "browser_action": {
    "default_icon": { 
      "19": "icon19.png"
    },
    "default_title": "cookie monster",
    "default_popup": "popup.html"
  }
}